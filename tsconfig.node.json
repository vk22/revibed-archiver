{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "jsx": "preserve",
    "moduleResolution": "node",
    "composite": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true
  },
  "include": [
    "src/main",
    "src/renderer/assets/preload",
    "package.json",
    "vite.config.mts",
    "buildAssets/builder"
  ]
}
